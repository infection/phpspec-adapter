{
    "license": "proprietary",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../.."
        }
    ],
    "autoload": {
        "psr-4": {
            "Infection\\PhpSpecAdapter\\E2ETests\\PhpSpec\\": "src"
        },
        "files": [
            "src/Covered/functions.php",
            "src/Uncovered/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Infection\\PhpSpecAdapter\\E2ETests\\PhpSpec\\": "spec",
            "Infection\\PhpSpecAdapter\\E2ETests\\PhpSpec\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "infection/infection": "^0.29.14 || ^1.0",
        "infection/phpspec-adapter": "@dev",
        "phpspec/phpspec": "^7.6 || ^8.0",
        "friends-of-phpspec/phpspec-code-coverage": "*",
        "phpunit/phpunit": "^11.5"
    }
}
